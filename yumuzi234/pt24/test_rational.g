func TestRational() {
    var r1, r2 RationalNum
    r1.Set(1, 2)
    r2.Set(1, 3)
    r1.Add(&r2)
    assert(r1.n == 5 && r1.d == 6)
    r1.AddInt(-4)
    assert(r1.n == -19 && r1.d == 6)
    r2.Set(3, 6)
    r1.Sub(&r2)
    assert(r1.n == -11 && r1.d == 3)
    r2.Set(2, 33)
    r1.Mul(&r2)
    assert(r1.n == -2 && r1.d == 9)
    r1.DivInt(-2)
    assert(r1.n == 1 && r1.d == 9)
    r2.Set(-1, 90)
    r1.Div(&r2)
    assert(r1.n == -10 && r1.d == 1)
    r1.SubInt(-10)
    assert(r1.n == 0 && r1.d == 1)
    r1.DivInt(-100)
    assert(r1.n == 0 && r1.d == 1)
}
